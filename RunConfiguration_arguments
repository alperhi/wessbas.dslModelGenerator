specj:
------

flows.add:

-w "./configuration/specj/workloadIntensity.properties" -b "./configuration/specj/behaviorModels.properties" -f "./examples/specj/input/flows/flows.all/" -o "./examples/specj/output/models/flows.all/workloadmodel.xmi" -g "./examples/specj/output/models/flows.all/graph.dot" -q false

flows.subset:

-w "./configuration/specj/workloadIntensity.properties" -b "./configuration/specj/behaviorModels.properties" -f "./examples/specj/input/flows/flows.subset/" -o "./examples/specj/output/models/flows.subset/workloadmodel.xmi" -g "./examples/specj/output/models/flows.subset/graph.dot" -q false


carshare:
---------

flow.all:

-w "./configuration/carshare/workloadIntensity.properties" -b "./configuration/carshare/behaviorModels.properties" -f "./examples/carshare/input/flows/flows.all/" -o "./examples/carshare/output/models/flows.all/workloadmodel.xmi" -g "./examples/carshare/output/models/flows.all/graph.dot" -q true

flows.subset1:

-w "./configuration/carshare/workloadIntensity.properties" -b "./configuration/carshare/behaviorModels.properties" -f "./examples/carshare/input/flows/flows.subset1/" -o "./examples/carshare/output/models/flows.subset1/workloadmodel.xmi" -g "./examples/carshare/output/models/flows.subset1/graph.dot" -q true

flows.subset2:

-w "./configuration/carshare/workloadIntensity.properties" -b "./configuration/carshare/behaviorModels.properties" -f "./examples/carshare/input/flows/flows.subset2/" -o "./examples/carshare/output/models/flows.subset2/workloadmodel.xmi" -g "./examples/carshare/output/models/flows.subset2/graph.dot" -q true
